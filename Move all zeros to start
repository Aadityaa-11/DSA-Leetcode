// Move all zero to start and some change move all negative value to start without changing order 


        int n = nums.size();
        int NonZeroIndex = 0 ; 
        for(int i = 0 ; i<n ; i++){
            if(nums[i] ==  0){
                int temp = nums[i];
                int j = i;


                // shift non zero right and preserve the order
                while(j > NonZeroIndex){
                    nums[j] = nums[j-1];
                    j--;
                }
                nums[NonZeroIndex] = temp;
                NonZeroIndex++;

            }
        }
